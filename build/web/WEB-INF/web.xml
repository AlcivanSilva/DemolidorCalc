<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Autenticacao</servlet-name>
        <servlet-class>br.jah.control.servlets.Autenticacao</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Sair</servlet-name>
        <servlet-class>br.jah.control.servlets.Sair</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CadastrarUsuario</servlet-name>
        <servlet-class>br.jah.control.servlets.CadastrarUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CalcTempoSVC</servlet-name>
        <servlet-class>br.jah.control.servlets.CalcTempoSVC</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Suporte</servlet-name>
        <servlet-class>br.jah.control.servlets.Suporte</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DecisorCliente</servlet-name>
        <servlet-class>br.jah.control.servlets.DecisorCliente</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Autenticacao</servlet-name>
        <url-pattern>/Autenticacao</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Sair</servlet-name>
        <url-pattern>/Sair</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CadastrarUsuario</servlet-name>
        <url-pattern>/CadastrarUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CalcTempoSVC</servlet-name>
        <url-pattern>/CalcTempoSVC</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Suporte</servlet-name>
        <url-pattern>/Suporte</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DecisorCliente</servlet-name>
        <url-pattern>/DecisorCliente</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>